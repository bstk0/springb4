    <threadsafe>true</threadsafe>
    <runtime>java8</runtime>
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"></property>
    </system-properties>
    <sessions-enabled>true</sessions-enabled>
